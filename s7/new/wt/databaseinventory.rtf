\par }{\f1\fs20\insrsid15795942 DatabaseInventory.dtd
\par 
\par     <?xml version="1.0" encoding="UTF-8"?>
\par     <!ELEMENT DatabaseInventory (DatabaseName+)>
\par     <!ELEMENT DatabaseName (   GlobalDatabaseName
\par                              , OracleSID
\par                              , DatabaseDomain
\par                              , Administrator+
\par                              , DatabaseAttributes
\par                              , Comments)
\par     >
\par     <!ELEMENT GlobalDatabaseName (#PCDATA)>
\par     <!ELEMENT OracleSID          (#PCDATA)>
\par     <!ELEMENT DatabaseDomain     (#PCDATA)>
\par     <!ELEMENT Administrator      (#PCDATA)>
\par     <!ELEMENT DatabaseAttributes EMPTY>
\par     <!ELEMENT Comments           (#PCDATA)>
\par 
\par     <!ATTLIST Administrator       EmailAlias CDATA #REQUIRED>
\par     <!ATTLIST Administrator       Extension  CDATA #IMPLIED>
\par     <!ATTLIST DatabaseAttributes  Type       (Production|Development|Testing) #REQUIRED>
\par     <!ATTLIST DatabaseAttributes  Version    (7|8|8i|9i) "9i">
\par 
\par     <!ENTITY AUTHOR "Jeffrey Hunter">
\par     <!ENTITY WEB    "www.iDevelopment.info">
\par     <!ENTITY EMAIL  "jhunter@iDevelopment.info">
\par 
\par Tablespaces.xml
\par 
\par     <?xml version="1.0" encoding="UTF-8"?>
\par 
\par     <!DOCTYPE Tablespaces [
\par 
\par       <!--
\par        | Used to create all required tablespaces in the database to be used by the 
\par        | application.
\par        +-->
\par       <!ELEMENT Tablespaces (Tablespace+)>
\par 
\par         <!--
\par          | Any tablespace requires at least one Datafile and an optional (only one)
\par          | Extent Management clause.
\par          +-->
\par         <!ELEMENT Tablespace (Datafile+, ExtentMgt?)>
\par         <!ATTLIST Tablespace  name     CDATA                  #REQUIRED
\par                               type     (permanent|temporary)  "permanent"
\par                               logging  (true|false)           "true">
\par 
\par           <!--
\par            | The Datafile element requires a file name attribute and can define an 
\par            | optional "Auto Extend" clause.
\par            +-->
\par           <!ELEMENT Datafile (Autoextend?)>
\par           <!ATTLIST Datafile  name       CDATA         #REQUIRED
\par                               size       CDATA         "500M"
\par                               reusefile  (true|false)  "true"
\par                               autoextend (on|off)      "off">
\par 
\par             <!ELEMENT Autoextend EMPTY>
\par             <!ATTLIST Autoextend  next  CDATA  "500M"
\par                                   max   CDATA  "unlimited">
\par 
\par           <!--
\par            | An optional Extent Management clause to declare for each tablespace.
\par            +-->
\par           <!ELEMENT ExtentMgt (AllocMethod?)>
\par           <!ATTLIST ExtentMgt method (local|dictionary) "local">
\par 
\par             <!ELEMENT AllocMethod EMPTY>
\par             <!ATTLIST AllocMethod  method  (AUTOALLOCATE|UNIFORM) "AUTOALLOCATE"
\par                                    size    CDATA                  "1M">
\par 
\par       ]>
